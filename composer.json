{
  "name": "thomaslhotta/wp-separate-user-base",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress/phpunit",
        "version": "6.0.3",
         "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/wordpress-develop/archive/6.0.3.zip"
        }
      }
    }
  ],
  "require": {
    "composer/installers": "1.* || ^2.1"
  },
  "require-dev": {
    "wordpress/phpunit": "6.*",
    "wp-cli/wp-cli": "2.*",
    "phpunit/phpunit": "9.*",
    "yoast/phpunit-polyfills": "^1.0"
  },
  "scripts": {
    "tests": [
      "Composer\\Config::disableProcessTimeout",
      "phpunit"
    ]
  },
  "config": {
    "allow-plugins": {
      "wp-cli/autoload-splitter": true,
      "composer/installers": true
    }
  }
}
